<?xml version="1.0"?>
<extension 
	xmlns="http://ns.adobe.com/air/extension/23.0">
	<id>de.pagecon.bleane</id>
	<versionNumber>2</versionNumber>
	<platforms>
		<platform 
			name="Android-ARM">
			<applicationDeployment>
				<nativeLibrary>AndroidBleLib.jar</nativeLibrary>
				<initializer>de.pagecon.bleane.BleExtension</initializer>
				<finalizer>de.pagecon.bleane.BleExtension</finalizer>
			</applicationDeployment>
		</platform>
		<platform 
			name="iPhone-ARM">
			<applicationDeployment>
				<nativeLibrary>libiOSBleLib.a</nativeLibrary>
				<initializer>BleExtensionInitializer</initializer>
				<finalizer>BleExtensionFinalizer</finalizer>
			</applicationDeployment>
		</platform>
		<platform 
			name="iPhone-x86">
			<applicationDeployment>
				<nativeLibrary>libiOSBleLib.a</nativeLibrary>
				<initializer>BleExtensionInitializer</initializer>
				<finalizer>BleExtensionFinalizer</finalizer>
			</applicationDeployment>
		</platform>
		<platform 
			name="Windows-x86">
			<applicationDeployment>
				<nativeLibrary>WinBleANENative.dll</nativeLibrary>
				<initializer>ExtInitializer</initializer>
				<finalizer>ExtFinalizer</finalizer>
			</applicationDeployment>
		</platform>
		<platform 
			name="MacOS-x86">
			<applicationDeployment>
				<nativeLibrary>macBleLib.framework</nativeLibrary>
				<initializer>initializer</initializer>
				<finalizer>finalizer</finalizer>
			</applicationDeployment>
		</platform>
		<platform 
			name="MacOS-x86-64">
			<applicationDeployment>
				<nativeLibrary>macBleLib.framework</nativeLibrary>
				<initializer>initializer</initializer>
				<finalizer>finalizer</finalizer>
			</applicationDeployment>
		</platform>
		<platform 
			name="default">
			<applicationDeployment />
		</platform>
	</platforms>
</extension>

